{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Raycast",
  "include": [
    "src/**/*",
    "raycast-env.d.ts"
  ],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "es2022"
    ],
    "module": "es2022",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022"
  }
}
